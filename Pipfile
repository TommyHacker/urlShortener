verify_ssl = true
name = "pypi"

[packages]
flask = "*"
flask-cors = "*"
flask-sqlalchemy = "*"

[dev-packages]

[scripts]
dev = "bash -c \"export FLASK_ENV='development' && flask run\""
start = "waitress-serve --listen=*:4040 app:app"
test = "pytest ."
coverage = "pytest --cov-report term-missing --cov=."

[requires]
python_version = "3.10"
